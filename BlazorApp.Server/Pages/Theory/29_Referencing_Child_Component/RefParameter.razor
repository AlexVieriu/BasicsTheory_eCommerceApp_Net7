@page "/refparameter"

<input type="button" class="btn btn-primary m-2" value="Visable" @onclick="ChangeVisibility" />
<RefParameterChild @ref="Visibility" />


@code {
    public RefParameterChild? Visibility { get; set; }

    public void ChangeVisibility()
    {
        this?.Visibility?.SetVisibility();
    }
}